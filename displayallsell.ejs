          <style>
               body{margin: 0;}
              .maincontainer{
                       background-color: rgba(121, 74, 101, 0.505);
                       background-size: 100% 100%;
                       
                       width: 100%;
                       height: auto;
                       display: flex;
                       justify-content: center;
                       align-items: center;
                       font-family:  "Poppins";
                       }
          
                .box{
                      background: rgba(172, 45, 19, 0.891);
                      background-size: auto;
                      width: 100%;
                      height: 90%;
                      margin-top: 20px !important; 
                      margin-bottom: 20px !important;
                      border-radius: 10px;
                      padding: 20px;
          
                    }
          
                .heading{
                        display: flex;
                        flex-direction: row;
                        align-items: center;
                }
          
                .headingstyle{
                        margin: 10px;
                        font-size: 20px;
                        font-weight: bold;
                }

                .pic{
                  width:40px;
                  height:40px;
                  border-radius: 10px; 
                
                }
          
                .addpic{
                      width: 50px;
                      height: 50px;
                      border-radius: 10px;
               }
          
               .btncenter{
                  display: flex;
                  justify-content: center;
                }
          
                .btnsize{
                  margin-top: 5px;
                  width: 80%;
                }
          
          
               
          </style>
          <html>
                <head>
                  <link rel="preconnect" href="https://fonts.googleapis.com">
                  <link rel="preconnect" href="https://fonts.gstatic.com" crossorigin>
                  <link href="https://fonts.googleapis.com/css2?family=Poppins:ital,wght@0,100;0,200;0,300;0,400;0,500;0,600;0,700;0,800;0,900;1,100;1,200;1,300;1,400;1,500;1,600;1,700;1,800;1,900&display=swap" rel="stylesheet">
                   <link href="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/css/bootstrap.min.css" rel="stylesheet" integrity="sha384-QWTKZyjpPEjISv5WaRU9OFeRpok6YctnYmDr5pNlyT2bRjXh0JMhjY6hW+ALEwIH" crossorigin="anonymous">
                   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.bundle.min.js" integrity="sha384-YvpcrYf0tY3lHB60NNkmXc5s9fDVZLESaAA55NDzOxhy9GkcIdslK1eN7N6jIeHz" crossorigin="anonymous"></script>
                   <script src="https://cdn.jsdelivr.net/npm/@popperjs/core@2.11.8/dist/umd/popper.min.js" integrity="sha384-I7E8VVD/ismYTF4hNIPjVp/Zjvgyol6VFvRkX/vR+Vc4jQkC+hVqc2pM8ODewa9r" crossorigin="anonymous"></script>
                   <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.3/dist/js/bootstrap.min.js" integrity="sha384-0pUGZvbkm6XF6gxjEnlmuGrJXVbNuzT9qBBavbLwCsOGabYfZo0T0to5eqruptLy" crossorigin="anonymous"></script>
                </head>
            <body>
            <div class="maincontainer" style="height: 100%;">
              <div class="box" style=" height: auto; margin-top: 20px; margin-bottom: 20px;">
                           <div class="row">
                              <div class="col-12">
                                <div class="heading">
                                    <img src="/images/sellericon.jpg" width="60px" height="50px"/>
                                    <div class="headingstyle">Details List</div>
                                    <div style="margin-left: auto;">
                                      <a href="/supply/supply_interface"><img src="/images/sellpageicon.png" class="addpic"/></a>
                                    </div>
                                </div>
                              </div>
                            </div>

         <table class="table">
          <tr>
                <th>Sn</th>
                <th>Supplier Id</th>
                <th>Supplier</th>
                <th>Stateid</th>
                <th>Amount Detail</th>
                <th>Firmname</th>
                <th>Products</th>
                <th>Logo</th>                
                <th>Edit</th>
          </tr> 
         
          <% data.map((item,i)=>{ %>
            <tr>
              <td><%=i+1 %></td>
              <td><%=item.supplierid %></td>
              <td><%=item.suppliername %><br><%=item.address %></td>
              <td><%=item.statename %><br><%=item.cityname %></td>

       <% var d=new Date(item.advancedate)
                     var cd=d.getFullYear()+"/"+(d.getMonth()+1)+"/"+d.getDate() %>

              <td><%=item.dueamount %><br><%=item.advance %><br><%=cd %></td>
              <td><%=item.firmname %></td>
              <td><%=item.products %></td>
              <td><img src='/images/<%=item.logo %>' class="pic"/></td>
              <td><a href="/supply/show_supplier?supplierid=<%=item.supplierid %>"><button type="button" class="btn btn-warning">Update</button></a></td>
            </tr>
            <% }) %>
          </table>
     
              </div>
            </div>
         </body> 
          </html>
          
          